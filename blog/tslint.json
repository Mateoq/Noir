{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-any": true,
        "no-magic-numbers": true,
        "no-invalid-template-strings": true,
        "max-line-length": [
            false
        ],
        "no-require-imports": true,
        "object-literal-sort-keys": [
            false
        ],
        "trailing-comma": [
            false
        ],
        "comment-format": [
            "check-space",
            "check-uppercase"
        ]
    },
    "rulesDirectory": []
}